with [get-view "Location Pool" | get-tree] {
    key-type "M1+a"
    key-type Del
}

get-view "Model Explorer" | get-tree | select "sirius.test/mapping.ecore/mapping/mapping class diagram" | double-click
get-editor "mapping class diagram" | get-diagram -index 1 | get-edit-part -name "mapping class diagram" 
    | mouse-move 310 549 -height 900 -width 1252
with [get-view Mapping] {
    click
    get-combo -after [get-label "Mapping base:"] | select "Sirius test"
}
with [get-editor "mapping class diagram" | get-diagram -index 1] {
    with [get-edit-part -name "mapping class diagram"] {
        mouse-move 229 256 -height 900 -width 1252
        with [get-edit-part -name LocationContainer | get-edit-part 
            -className DNodeListViewNodeListCompartment2EditPart] {
            mouse-move 69 43 button3 -height 71 -width 116
            mouse-press 69 43 button3 -height 71 -width 116
            mouse-move 69 44 -height 71 -width 116
            mouse-hover 69 44 -height 71 -width 116
            mouse-move 69 44 button3 -height 71 -width 116
            mouse-press 69 44 button3 -height 71 -width 116
        }
    }
    get-menu "Mapping/Add Location to Pool" | click
    with [get-edit-part -name "mapping class diagram"] {
        mouse-move 533 478 -height 900 -width 1252
        get-edit-part -name Location | get-edit-part -className DNodeListViewNodeListCompartment2EditPart 
            | mouse-move 70 43 -height 71 -width 116
    }
}
get-view "Location Pool" | click
get-view "Location Pool" | get-tree | get-item LocationContainer | get-property caption | equals LocationContainer 
    | verify-true
